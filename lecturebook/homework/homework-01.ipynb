    "from scipy.optimize import fsolve\n",
    "p_A_given_BI = 0.99999\n",
    "p_B_given_I = 0.004\n",
    "# p_A_given_notB_I = 0.00001\n",
    "\n",
    "def p_B_given_AI(p_A_given_notB_I):\n",
    "    p_A_given_I = p_A_given_BI * p_B_given_I + p_A_given_notB_I * (1 - p_B_given_I)\n",
    "    p_B_given_AI = p_B_given_I * p_A_given_BI / p_A_given_I \n",
    "    return (p_B_given_AI-0.99)*10000\n",
    "\n",
    "sol = fsolve(p_B_given_AI, 0.001)\n",
    "sol[0]"
