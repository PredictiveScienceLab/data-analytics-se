    "# Your code here\n",
    "ts = np.linspace(0.0, 20.0, 100)\n",
    "fig, ax = plt.subplots(dpi=150)\n",
    "ax.plot(ts, T.cdf(ts))\n",
    "ax.set_xlabel(r\"$t$ (years)\")\n",
    "ax.set_ylabel(r\"$p(t)$\")\n",
    "ax.set_title(\"CDF of $T$\")\n",
    "sns.despine(trim=True);"
   ]
